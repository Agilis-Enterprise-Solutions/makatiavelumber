<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="po_stage_0" model="purchase.order.stages">
        <field name="sequence">0</field>
        <field name="name">RFQ</field>
        <field name="stage_key">draft</field>
        <field name="color">1</field>
    </record>
    <record id="po_stage_1" model="purchase.order.stages">
        <field name="sequence">1</field>
        <field name="name">RFQ Sent</field>
        <field name="stage_key">sent</field>
        <field name="color">2</field>
    </record>
    <record id="po_stage_2" model="purchase.order.stages">
        <field name="sequence">2</field>
        <field name="name">To Approve</field>
        <field name="stage_key">to approve</field>
        <field name="color">3</field>
    </record>
    <record id="po_stage_3" model="purchase.order.stages">
        <field name="sequence">3</field>
        <field name="name">Purchase Order</field>
        <field name="stage_key">purchase</field>
        <field name="color">4</field>
    </record>
    <record id="po_stage_4" model="purchase.order.stages">
        <field name="sequence">4</field>
        <field name="name">Locked</field>
        <field name="stage_key">done</field>
        <field name="color">5</field>
    </record>
    <record id="po_stage_5" model="purchase.order.stages">
        <field name="sequence">5</field>
        <field name="name">Cancelled</field>
        <field name="stage_key">cancel</field>
        <field name="color">6</field>
    </record>
</odoo>
